class Solution {
    public String[] reorderLogFiles(String[] logs) {
            int n = logs.length;
        //identifier : the first word
        
        List<String> letter = new ArrayList<>();
        List<String> digit = new ArrayList<>();
        
        //letter , digit 분리
        for(int i = 0 ; i < n ; i++) {
        	String[] identifier = logs[i].split(" ");
            //공백으로 구분 후, 두번째 단어의 영어인지 숫자인지에 따른 각각의 list에 add
        	if(Character.isDigit(identifier[1].charAt(0))) digit.add(logs[i]);
        	else letter.add(logs[i]);
        }
        
        //letter sort -> 오름차순
       Collections.sort(letter, new Comparator<String>() {
    	   public int compare(String o1 , String o2) {
               //두번째 단어 기준
    		   String s1 = o1.substring(o1.indexOf(" ")+1); 
    		   String s2 = o2.substring(o2.indexOf(" ")+1);
               
    		   return s1.equals(s2) ? o1.compareTo(o2) : s1.compareTo(s2);
    	   }
       });
      


        String[] ans = new String[letter.size()+digit.size()];
        for(int i = 0 ; i < letter.size() ; i++) {
        	ans[i] = letter.get(i);
        }
        int j = 0;
        for(int i = letter.size() ; i < ans.length ; i++) {
        	ans[i] = digit.get(j++);
        }
        
        return  ans;
    }
}
